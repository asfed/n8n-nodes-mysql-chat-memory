{
    "compilerOptions": {
	"target": "es2019",
	"module": "commonjs",
	"lib": [
	    "es2019"
	],
	"strict": true,
	"esModuleInterop": true,
	"sourceMap": true,
	"skipLibCheck": true,
	"forceConsistentCasingInFileNames": true,
	"outDir": "./dist",
	"types": [
	    "node"
	]
    },
    "include": [
	"credentials/**/*",
	"nodes/**/*",
    ]
}